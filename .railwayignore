# Railway Ignore File
# Tells Railway to ignore frontend/React code and only deploy the cron server

# Ignore frontend source code
src/
public/
index.html
vite.config.ts
tsconfig.json

# Ignore development files
.git/
.github/workflows/
node_modules/
dist/
build/

# Ignore other services
screenshot-service/

# Keep only what we need for Railway server
!railway-server.ts
!package.json
!railway.json

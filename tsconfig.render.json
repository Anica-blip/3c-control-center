{
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["ES2023"],
    "module": "ES2022",
    "moduleResolution": "bundler",
    "outDir": "./dist",
    "rootDir": "./src",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true
  },
  "include": [
    "src/schedulecomponent/api/cronHandler.ts",
    "src/schedulecomponent/config.node.ts"
  ],
  "exclude": ["node_modules", "dist"]
}
